model Like {
    userId    String
    songId    String
    createdAt DateTime @default(now()) @map("created_at")

    user User @relation(fields: [userId], references: [id])
    song Song @relation(fields: [songId], references: [id])

    @@id([userId, songId])
    @@index([createdAt])
    @@map("likes")
}
